{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": ["dom", "esnext"],
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "target": "es5",
    "esModuleInterop": true,
    "downlevelIteration": true,
    "useDefineForClassFields": true
  },
  "extends": "expo/tsconfig.base"
}
